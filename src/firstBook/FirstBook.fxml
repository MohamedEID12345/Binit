<AnchorPane id="AnchorPane" prefHeight="431.0" prefWidth="462.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="firstBook.FirstBookController">
    <stylesheets>
        <URL value="@firstbook.css" />
